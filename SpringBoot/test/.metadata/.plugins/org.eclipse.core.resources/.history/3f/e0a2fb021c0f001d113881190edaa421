package com.test.demo.entity;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

import lombok.Data;

@Data
@Table(name="countries")
@Entity
public class country {
	@Id
	@Column(value="id")
	private long id;
	
	@Column(value="neame")
	private String name;
}	
